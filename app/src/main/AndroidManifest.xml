<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.organizzeclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OrganizzeClone">
        <activity android:name=".ui.DespesaActivity" android:theme="@style/despesaTheme"></activity>
        <activity android:name=".ui.ReceitaActivity"  android:theme="@style/receitaTheme"/>
        <activity
            android:name=".ui.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.OrganizzeClone.NoActionBar" />
        <activity android:name=".ui.CadastroActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>